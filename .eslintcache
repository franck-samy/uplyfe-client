ord}\n            onChange={this.handleInputChange}\n            required\n            minLength=\"8\"\n          />\n\n          {this.state.error && (\n            <div className=\"error-block\">\n              <p>There was an error submiting the form:</p>\n              <p>{this.state.error.message}</p>\n            </div>\n          )}\n\n          <button className=\"button__submit\" type=\"submit\">\n            Submit\n          </button>\n        </form>\n      </div>\n    );\n  }\n}\n","/Users/francksamy/Desktop/Uplyf/Uplyf/client/src/routing-components/ProtectedRoute.jsx",[],"/Users/francksamy/Desktop/Uplyf/Uplyf/client/src/components/Navbar/Navbar.jsx",["90","91","92"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nimport \"./Navbar.css\";\n// const userimg = url(\"../../../public/images/user.png\");\n\nconst Navbar = (props) => {\n  return (\n    <nav className=\"navbar\">\n      <Link to=\"/profile\" className=\"navbar\">\n        <img\n          className=\"navButtons\"\n          src=\"../../../images/user.png\"\n          alt=\" user image\"\n        />\n      </Link>\n      <Link to=\"/feed\" className=\"navbar\">\n        <img\n          className=\"navButtons\"\n          src=\"../../../images/idea.png\"\n          alt=\" feed image\"\n        />\n      </Link>\n      <Link to=\"/followers\" className=\"navbar\">\n        <img\n          className=\"navButtons\"\n          src=\"../../../images/followers.png\"\n          alt=\"follower image\"\n        />\n      </Link>\n\n      {/* <div className=\"nav__authLinks\">\n        {props.user ? (\n          <>\n            <Link to=\"/protected\" className=\"authLink\">\n              Protected Page\n            </Link>\n            <button className=\"nav-logoutbtn\" onClick={props.handleLogout}>\n              Logout\n            </button>\n          </>\n        ) : (\n          <>\n            <Link to=\"/auth/signup\" className=\"authLink\">\n              Signup\n     